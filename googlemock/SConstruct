"""
Scons build script for google mock library
"""
Import("*")

env.AppendUnique(CPPPATH = ['#/googletest/include'])
env.AppendUnique(CPPPATH = ['#/googlemock'])
env.AppendUnique(CPPPATH = ['#/googlemock/include'])

env.Library('gmock', Glob('src/*.cc'))
